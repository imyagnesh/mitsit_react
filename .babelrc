{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-private-methods",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "pages": "./src/pages",
          "assets": "./src/assets",
          "layout": "./src/layout",
          "context": "./src/context",
          "components": "./src/components",
          "utils": "./src/utils",
          "reducers": "./src/reducers",
          "actions": "./src/actions"
        }
      }
    ]
  ]
}
